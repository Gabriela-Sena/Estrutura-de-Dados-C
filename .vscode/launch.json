{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "enter program name, for example ${workspaceFolder}/a.exe",
            "MIMode": "gdb",
            "miDebuggerPath": "/path/to/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
          "name": "Launch Program",
          "type": "cppdbg",
          "request": "launch",
          "program": "C:/Users/Baltazar/Documents/GitHub/Estrutura-de-Dados-C/Ponteiros/exemplo1.exe", // Caminho para o executável
          "args": [], // Argumentos do programa (se houver)
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": true,
          "MIMode": "gdb",
          "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            }
          ],
          "preLaunchTask": "C/C++: gcc.exe build active file", // Tarefa de compilação a ser executada antes do lançamento
          "miDebuggerPath": "C:/MinGW/64bits/MinGW 4.8.5/mingw64/bin" // Caminho para o depurador (gdb)
        }
      ]
}